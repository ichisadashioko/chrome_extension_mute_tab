{
    "name": "mute_tab",
    "version": "0.0.01",
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "permissions": [
        "activeTab",
        "contextMenus"
    ],
    "browser_action": {
        "default_title": "toggle_mute"
    },
    "commands": {
        "toggle_mute": {
            "suggested_key": {
                "default": "Ctrl+M"
            },
            "description": "toggle_mute"
        }
    },
    "manifest_version": 2
}
